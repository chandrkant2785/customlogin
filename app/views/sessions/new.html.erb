<div class="container_1">
	<section id="content">
<h1>Log in</h1>

<%= form_tag sessions_path do %>
  
   <div> <%= label_tag :email %></div><br />
  <div>  <%= text_field_tag :email, params[:email] %></div>
  
    <div><%= label_tag :password %></div><br />
   <div> <%= password_field_tag :password %></div>
  <div><%= submit_tag "Log in" %></div>
<% end %>

</section>
</div>